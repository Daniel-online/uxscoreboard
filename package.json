{
  "name": "uxscoreboard",
  "version": "1.0.0",
  "description": "live sports scoreboard built on react.js",
  "main": "server.js",
  "scripts": {
    "start": "env UV_THREADPOOL_SIZE=100 webpack-dev-server --progress",
    "build": "webpack -p --progress",
    "node": "node server.js",
    "server": "browser-sync start --directory --server --files '*js,*.css,*.html'",
    "lint": "eslint app/.; exit 0",
    "fix": "eslint --fix app/.; exit 0",
    "svgo": "find ./dist/assets -name '*.svg' -type f -exec svgo '{}' \\;"
  },
  "author": "Zac Arellano",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "moment-timezone": "^0.5.9",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-icons": "^2.2.1",
    "react-router": "^2.0.0-rc5",
    "velocity-react": "^1.1.11"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.18.2",
    "browser-sync-webpack-plugin": "^1.1.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "html-webpack-plugin": "^2.21.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^1.1.1",
    "style-loader": "^0.13.1",
    "svgo": "^0.7.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zacarellano/uxscoreboard.git"
  },
  "bugs": {
    "url": "https://github.com/zacarellano/uxscoreboard/issues"
  },
  "homepage": "https://github.com/zacarellano/uxscoreboard#readme"
}
